<Window x:Class="policripsysoftware.KelolaPengguna"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:policripsysoftware"
        mc:Ignorable="d"
        Title="kpw" Height="510" Width="626.667">
    <Grid Background="Black">
        <DockPanel HorizontalAlignment="Left" Height="57" VerticalAlignment="Top" Width="618.667">
            <Label Content="Kelola Pengguna" HorizontalAlignment="Center" VerticalAlignment="Center" Width="159" Margin="0,10" Height="37" Foreground="White" FontSize="20"/>
        </DockPanel>
        <StackPanel HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="295" Margin="0,57,0,0">
            <Label Content="Pasien" HorizontalAlignment="Center" VerticalAlignment="Top" RenderTransformOrigin="3.789,0.808" FontWeight="Bold" Foreground="White"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="323.667" Margin="295,57,0,0">
            <Label Content="Dokter" HorizontalAlignment="Center" VerticalAlignment="Top" RenderTransformOrigin="3.789,0.808" FontWeight="Bold" Foreground="White"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="216" VerticalAlignment="Top" Width="162" Margin="0,83,0,0">
            <Label Content="Nama Pasien" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="-1.513,-1.462" Foreground="White" Margin="40,0,0,10" Width="100" Height="25"/>
            <Label Content="Tanggal Lahir" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="-1.988,-3.462" Foreground="White" Margin="40,0,0,10" Width="100" Height="25"/>
            <Label Content="Jenis Kelamin" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" Margin="40,0,0,10" Width="100" Height="25" RenderTransformOrigin="0.467,0.143"/>
            <Label Content="No Hp" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" Margin="40,0,0,10" Width="100" Height="26"/>
            <Label Content="Alamat" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" Margin="40,0,0,10" Width="100" Height="26"/>
            <Label Content="Dokter" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" Margin="40,0,0,10" Width="100" Height="25"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="216" VerticalAlignment="Top" Width="152" Margin="162,83,0,0">
            <TextBox x:Name="namatxt" Height="24" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" HorizontalAlignment="Left" Margin="40,0,0,10"/>
            <DatePicker x:Name="tanggallahirtxt" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="40,0,0,10" Height="24"/>
            <ComboBox x:Name="Jenisklamintxt" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="40,0,0,10" Height="24">
                <ComboBoxItem Content="Pria" HorizontalAlignment="Left" Width="118"/>
                <ComboBoxItem Content="Wanita" HorizontalAlignment="Left" Width="118"/>
            </ComboBox>
            <TextBox x:Name="nohptxt" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="40,0,0,10"/>
            <TextBox x:Name="alamattxt" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="40,0,0,10"/>
            <ComboBox x:Name="doktertxt" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="40,0,0,10" Height="24"/>
        </StackPanel>

        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="109" Margin="-3,304,0,0" VerticalAlignment="Top" Width="298"/>
        <Button x:Name="btv" Content="View" HorizontalAlignment="Left" Margin="174,440,0,0" VerticalAlignment="Top" Width="40" Click="Btv_Click"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="14,440,0,0" VerticalAlignment="Top" Width="41"/>
        <Button Content="Update" HorizontalAlignment="Left" Margin="98,440,0,0" VerticalAlignment="Top" Width="41"/>
        <Button x:Name="DeleteButtonPasien" Content="Delete" HorizontalAlignment="Left" Margin="254,440,0,0" VerticalAlignment="Top" Width="41" Click="DeleteButtonPasien_Click"/>
        <DataGrid x:Name="datagriddokter" Height="109" VerticalAlignment="Top" Margin="327.667,304,0,0"/>
        <Button x:Name="viewdokter" Content="View" HorizontalAlignment="Left" Margin="487.667,440,0,0" VerticalAlignment="Top" Width="40" Click="Btv_Click"/>
        <Button x:Name="adddokter" Content="Add" HorizontalAlignment="Left" Margin="327.667,440,0,0" VerticalAlignment="Top" Width="41" Click="Adddokter_Click"/>
        <Button x:Name="updatedokter" Content="Update" HorizontalAlignment="Left" Margin="411.667,440,0,0" VerticalAlignment="Top" Width="41"/>
        <Button x:Name="deletedokter" Content="Delete" HorizontalAlignment="Left" Margin="567.667,440,0,0" VerticalAlignment="Top" Width="41" RenderTransformOrigin="0.366,5.21"/>
        <StackPanel HorizontalAlignment="Left" Height="216" VerticalAlignment="Top" Width="138.667" Margin="314,83,0,0">
            <Label Content="Nama Dokter" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="-1.513,-1.462" Foreground="White" Margin="40,0,-1,10" Width="100" Height="25"/>
            <Label Content="Spesialis" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="-1.988,-3.462" Foreground="White" Margin="40,0,-1,10" Width="100" Height="25"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="216" VerticalAlignment="Top" Width="161" Margin="457.667,83,0,0">
            <TextBox x:Name="namadoktertxt" Height="24" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" HorizontalAlignment="Left" Margin="40,0,0,10"/>
            <TextBox x:Name="spesialistxt" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="40,0,0,10"/>
        </StackPanel>

    </Grid>
</Window>
